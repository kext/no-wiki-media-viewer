{
  "manifest_version": 2,
  "name": "No Wiki Media Viewer",
  "version": "1.0.0",
  "author": "Kext",
  "description": "Disables the Media Viewer on all Wikipedia pages.",
  "homepage_url": "https://github.com/kext/no-wiki-media-viewer",
  "content_scripts": [
    {
      "matches": ["*://*.wikipedia.org/*", "*://*.wikimedia.org/*"],
      "js": ["no-wiki-media-viewer.js"]
    }
  ]
}
